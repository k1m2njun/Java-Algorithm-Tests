package boj02439;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		scanner.close();
		
		for(int i = 0; i < n; i++) {
			for( int j = 0; j < i+1; j++) {
				if(j<4) {
					System.out.print(' ');
				} else {
					System.out.print('*');
				}
			}
			System.out.println();			
		}
	}

}
