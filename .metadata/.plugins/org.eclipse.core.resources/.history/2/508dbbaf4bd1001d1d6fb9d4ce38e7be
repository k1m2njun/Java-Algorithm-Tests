package boj11720;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		String s = scanner.next();
		int sum = 0;
		
		for(int i = 0; i < n; i++) {
			sum += s.charAt(i)-48;
		}
		System.out.println(sum);
	}

}
